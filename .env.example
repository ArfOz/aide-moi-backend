# Environment variables
NODE_ENV=development
PORT=3000
HOST=0.0.0.0

# Database Configuration
DB_TYPE=sqlite
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=your_username
DB_PASSWORD=your_password
